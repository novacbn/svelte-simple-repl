{
    "name": "svelte-simple-repl",
    "version": "0.0.1",
    "description": "Simple Svelte v3 Component for providing an end-user editable view for real-time editing of Svelte code",
    "keywords": [
        "repl",
        "svelte"
    ],
    "files": [
        "dist"
    ],
    "author": "novacbn",
    "repository": {
        "type": "git",
        "url": "https://github.com/novacbn/svelte-simple-repl"
    },
    "license": "MIT",
    "engines": {
        "node": ">=6.0.0"
    },
    "scripts": {
        "build:lib": "npm run build:lib:browser",
        "build:lib:browser": "parcel build ./src/index.js --experimental-scope-hoisting --global SimpleRepl --out-file ./dist/svelte-simple-repl.umd.js"
    },
    "dependencies": {
        "svelte": "^3.16.5",
        "svelte-ace-editor": "0.0.2"
    },
    "devDependencies": {
        "parcel-bundler": "^1.12.4",
        "parcel-plugin-svelte": "^4.0.5",
        "prettier": "^1.14.3"
    }
}
